[include]
  path = ~/.gitconfig_local
[push]
  default = current
[color]
  diff   = auto
  status = auto
  branch = auto
  grep   = auto
  ui     = auto
[core]
  excludesfile   = ~/.gitignore_global
  editor         = vim
  attributesfile = ~/.gitattributes_global
[alias]
  st    = status
  co    = checkout
  di    = diff
  br    = branch
  addd  = add
  puhs  = push
  ush   = push
  psuh  = push
  ull   = pull
  comit = commit
  pr    = pull --rebase
  ch    = cherry-pick
[filter "media"]
  clean  = git-media-clean %f
  smudge = git-media-smudge %f
[credential]
	UseHttpPath = true
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
