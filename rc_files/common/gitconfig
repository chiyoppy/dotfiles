[include]
    path = .gitconfig_local
[push]
    default = current
[color]
    diff   = auto
    status = auto
    branch = auto
    grep   = auto
    ui     = auto
[core]
    excludesfile   = .gitignore
    editor         = vim
    attributesfile = .gitattributes_global
[alias]
    st   = status
    co   = checkout
    ci   = commit -v
    di   = diff
    br   = branch
[filter "media"]
	clean  = git-media-clean %f
	smudge = git-media-smudge %f
